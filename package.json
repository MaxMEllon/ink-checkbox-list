{
	"name": "ink-checkbox-list",
	"version": "1.3.0",
	"main": "lib/index.js",
	"description": " Checkbox list component for ink.",
	"repository": "MaxMEllon/ink-checkbox-list",
	"author": "MaxMEllon <maxmellon1994@gmail.com>",
	"license": "MIT",
	"engines": {
		"node": ">=6"
	},
	"files": [
		"lib"
	],
	"keywords": [
		"ink",
		"text",
		"input",
		"component",
		"jsx",
		"react",
		"stdin"
	],
	"scripts": {
		"build": "babel src --out-dir lib",
		"release": "npm run build && npm test && npm publish",
		"pretest": "npm run build",
		"test": "xo && ava"
	},
	"babel": {
		"presets": [
			"@babel/preset-react",
			"@babel/preset-env"
		]
	},
	"ava": {
		"babel": {
			"presets": [
				"@babel/preset-react",
				"@babel/preset-env"
			]
		}
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"rules": {
			"react/prop-types": 0,
			"react/jsx-key": 0,
			"react/no-unused-prop-types": 0,
			"react/require-default-props": 0
		},
		"ignores": [
			"example/*.js"
		],
		"settings": {
			"react": {
				"pragma": "h"
			}
		}
	},
	"devDependencies": {
		"ava": "^0.20.0",
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.3",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-react": "^7.0.0",
		"eslint-config-xo-react": "^0.13.0",
		"eslint-plugin-react": "^7.1.0",
		"ink": "^2.0.0",
		"sinon": "^2.3.7",
		"standard": "^10.0.2",
		"xo": "^0.18.2"
	},
	"dependencies": {
		"figures": "^2.0.0",
		"prop-types": "^15.5.10",
		"react": "^16.8.6"
	}
}
